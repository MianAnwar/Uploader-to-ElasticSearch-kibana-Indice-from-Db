    public void JSONPostData(string filename)
    {
        try
        {
            string[] files = Directory.GetFiles(Path.GetDirectoryName(filename), index + "_*.txt");
            foreach (string file in files)
            { 
                StreamReader sr = new StreamReader(file);
                string data = sr.ReadToEnd();
                sr.Dispose();
                byte[] bytes = Encoding.UTF8.GetBytes(data);


                /* string authInfo = username + ":" + password;
                 HttpWebRequest request = (HttpWebRequest)WebRequest.Create(uri);
                 request.Method = "GET";
                 request.Accept = "application/json; charset=utf-8";
                 request.Proxy = proxy; 
                 request.Headers["Authorization"] = "Basic " + authInfo;*/

                string uri = url + @"/_bulk";
                string authInfo = username + ":" + password;
                NetworkCredential nc = new NetworkCredential(username, password);
               CredentialCache cache = new CredentialCache();

                cache.Add(new Uri(uri) , "Basic", nc);



                HttpWebRequest webrequest = WebRequest.Create(url + @"/_bulk") as HttpWebRequest;
                webrequest.Timeout = Timeout.Infinite;
                webrequest.KeepAlive = true;
                webrequest.Credentials = cache;
                //webrequest.Headers["Authorization"] = "Basic " + authInfo; 
                //webrequest.KeepAlive = false;
                webrequest.Method = "POST";
                webrequest.ContentType = "application/json";
                HttpWebResponse webresponse1 = null;
                string response1 = null;
                try
                {
                    using (var dataStream = webrequest.GetRequestStream())
                    {
                        dataStream.Write(bytes, 0, bytes.Length);
                    }


                    using (HttpWebResponse webresponse = (HttpWebResponse)webrequest.GetResponse())
                    {                       
                        webresponse1 = webresponse;
                        using (StreamReader resp = new StreamReader(webresponse.GetResponseStream()))
                        {
                            string response = (resp.ReadToEnd()).ToString();
                            response1 = response;
                            CheckJSONResponse(filename, response.Replace("\r\n", "").Replace("\n", "").Replace("\r", ""));
                        }
                    }
                    //HttpWebResponse webresponse = (HttpWebResponse)webrequest.GetResponse();
                    //webresponse1 = webresponse;
                    //using (StreamReader resp = new StreamReader(webresponse.GetResponseStream()))
