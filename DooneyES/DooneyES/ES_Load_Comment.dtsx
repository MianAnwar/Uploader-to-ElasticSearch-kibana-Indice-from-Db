        }

    }

    public void CheckJSONResponse(string filename, string response)
    {
        RootObject jsonResponse1 = null;
        try
        {
            //SQL Connection
            string constr = "";
            string[] conn = SQLConnString.ToString().Split(';');

            foreach (string val in conn)
            {
                if (val.ToLower().Contains("source") || val.ToLower().Contains("catalog") || val.ToLower().Contains("database") || val.ToLower().Contains("security"))
                    constr += val + ";";
            }

            int start = response.IndexOf('{');
            int len = response.Length;
            string res = response.Substring(start, len - start);

            DataContractJsonSerializer deserializer = new DataContractJsonSerializer(typeof(RootObject));
            RootObject jsonResponse = (RootObject)deserializer.ReadObject(new MemoryStream(Encoding.Unicode.GetBytes(res)));
            jsonResponse1 = jsonResponse;

            //if (jsonResponse.errors.Equals(true))
            //{

                SqlConnection SQLConnection = new SqlConnection();
                SQLConnection.ConnectionString = constr;
                SQLConnection.Open();
                foreach (Item item in jsonResponse.items)
                {
                    if (item.index.status.Equals(400))
                    {
                        string query = "INSERT INTO [dbo].[ES_Log]([ProcessDetailID],[PackageName],[JSONFileName],[IndexName],[IndexType],[SourceTable],[DocumentID],[ErrorType],[ErrorReason]) VALUES('" + ETLAuditID + "','" + packagename + "','" + filename + "','" + item.index._index.ToString() + "','" + item.index._type.ToString() + "','" + item.index._index.ToString() + "','" + item.index._id.ToString() + "','" + item.index.error.caused_by.type.ToString().Replace("'", "") + "','" + item.index.error.caused_by.reason.ToString().Replace("'", "") + "')";
                        //sqlcommand to insert record
                        SqlCommand myCommand = new SqlCommand(query, SQLConnection);
                        myCommand.ExecuteNonQuery();
                        FailedRecord++;

                    }
                    else
                    {
                        if (item.index._version.Equals(1))
                        {
                            insertedRecord++;
                        }
                        else if (item.index.result.ToLower().Equals("updated".ToLower()))
                        {
                            updatedRecord++;
                        }
                    }
                    
                    
                }
                SQLConnection.Close();
        }
        catch(Exception ex)
        {
           // System.IO.File.AppendAllText("E:\\Ticketing\\error.txt", ex.Message + ex.StackTrace + ex.Data + ex.ToString() + "check json response exception wala exception" + " response " + response.ToString() + "jsonResponse.items:  " + jsonResponse1.items.ToString() + jsonResponse1.errors.ToString());
            throw ex;
        }


       
    }

    public class RootObject
    {
        public int took { get; set; }
        public bool errors { get; set; }
        public List<Item> items { get; set; }
    }
    public class Item
    {
        public Index index { get; set; }
    }
    public class Index
    {
        public string _index { get; set; }
        public string _type { get; set; }
        public string _id { get; set; }
        public int _version { get; set; }
        public string result { get; set; }
        public Shards _shards { get; set; }
        public bool created { get; set; }
        public int status { get; set; }
        public Error error { get; set; }
    }
    public class Shards
    {
        public int total { get; set; }
        public int successful { get; set; }
        public int failed { get; set; }
    }
    public class Error
    {
        public string type { get; set; }
        public string reason { get; set; }
        public CausedBy caused_by { get; set; }
    }
    public class CausedBy
    {
        public string type { get; set; }
        public string reason { get; set; }
    }
}


