    /// <summary>
    /// This method is called once, before rows begin to be processed in the data flow.
    ///
    /// You can remove this method if you don't need to do anything here.
    /// </summary>
    string Filedir;
    int filenum;
    string index;
    int rownum;
    long jsondoc;
    public override void PreExecute()
    {
        base.PreExecute();
        filenum = 1;
        rownum = 1;
        index = this.Variables.Indexname.ToString();
        Filedir = this.Variables.JSONFileDir + "\\" + this.Variables.folder.ToString().ToUpper() + "\\";
        jsondoc = this.Variables.JSONDocCount;
        //Delete and Create directory for first time
        if (rownum == 1 && Directory.Exists(Filedir))
        {
            Directory.Delete(Filedir, true);
        }
        if (rownum == 1 && !(Directory.Exists(Filedir)))
        {
            Directory.CreateDirectory(Filedir);
        }
    }

    /// <summary>
    /// This method is called after all the rows have passed through this component.
    ///
    /// You can delete this method if you don't need to do anything here.
    /// </summary>
    public override void PostExecute()
    {
        base.PostExecute();

        /*
         * Add your code here
         */
    }


    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        try
        {
            string indexdocid = ReplaceCharacter(Row.OrderNumber + "-" + Row.CreationDate);
            string indexname = index.ToLower();
            //string indextype = "mdm_type";

            //Create JSON for ES
            //document line1
            //string line1 = "{\"index\" : { \"_index\" : \"" + indexname + "\", \"_type\" : \"" + indextype + "\", \"_id\" : \"" + indexdocid + "\" } }";
             string line1 = "{\"index\" : { \"_index\" : \"" + indexname + "\", \"_id\" : \"" + indexdocid + "\" } }";
            string doc = "";
            //document rowdata

            doc = "{ " +
                        "\"OrderNumber\" : \"" + ReplaceCharacter(Row.OrderNumber) + "\"" + "," +
                        "\"CreationDate\" : \"" + (Row.CreationDate_IsNull ? null : ReplaceCharacter(Row.CreationDate)) + "\"" + "," +
                        "\"Note\" : \"" + (Row.Note_IsNull ? null : ReplaceCharacter(Row.Note)) + "\"" + ",";

            doc += "\"esModifiedDateTime\" : \"" + Row.ESMODIFIEDDATETIME.ToString("yyyy-MM-dd") + "\" }";



            string output = line1 + "\n" + doc + "\n";

            string filename = Filedir + indexname + "_" + filenum + ".txt";

            //Change the fime number
            if (rownum % jsondoc == 0)
            {
                filenum = filenum + 1;
            }

            //Output fields
            Row.output = output;
            Row.filename = filename;
            Row.rownum = rownum;
            rownum++;

        }
        catch (Exception ex)
        {
            //Dts.Events.FireError(0, "Script Task Example", ex.Message + "\r" + ex.StackTrace, String.Empty, 0);
            //Dts.TaskResult = (int)ScriptResults.Failure;
        }
    }
    public string ReplaceCharacter(string s)
    {
        if (!(s == null || s == string.Empty))
        {
            string rep = s.Replace("\\", "\\\\").Replace("\"", "\\\"").Replace("\r\n", "<br />").Replace("\n", "<br />").Replace("\r", "<br />").Replace("\t", "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;");
            return rep;
        }
        else
            return s;

    }

    public string ReplaceCharacterPhone(string s)
    {
        if (!(s == null || s == string.Empty))
        {
            string rep = s.Replace("-", "").Replace("\r\n", "<br />").Replace("\n", "<br />").Replace("\r", "<br />");
            return rep;
        }
        else
            return s;

    }

}
