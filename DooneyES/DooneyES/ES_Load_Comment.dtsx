               //  System.IO.File.AppendAllText("E:\\Ticketing\\error.txt", "abcdefghijklmnopqrstuvwxyz" + "webrequest: " + webrequest.ToString() + "webresponse: " + webresponse1.ToString() + "Response: " + response1.ToString());
                    throw ex;
                }

                //Delete file after posting in ES
                File.Delete(file);
            }
            
        }
        catch (WebException webex)
        {
         //  System.IO.File.AppendAllText("E:\\Ticketing\\error.txt", webex.Message + webex.StackTrace + webex.Data + "+++++4");
            WebResponse errResp = webex.Response;
